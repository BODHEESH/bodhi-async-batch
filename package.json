{
  "name": "bodhi-async-batch",
  "version": "1.0.2",
  "description": "Run asynchronous tasks in batches with controlled concurrency - perfect for API calls, database operations, and file processing",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:js && npm run build:ts",
    "build:js": "if not exist dist mkdir dist && copy /Y ./src/index.js ./dist/index.js && node tools/create-mjs.js",
    "build:ts": "tsc",
    "test": "jest --coverage",
    "lint": "eslint src tests",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
